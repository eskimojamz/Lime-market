                      onClick={() => setToggleCarousel(!toggleCarousel)}\n                    >\n                        Close\n                    </button>\n                </motion.div>\n            </motion.div>\n            </AnimatePresence>\n        </>\n    )\n}\n\nexport default ImageCarousel\n","/Users/eskimo/Coding/Lime-market/client/src/components/ScrollToTop.js",[],"/Users/eskimo/Coding/Lime-market/client/src/assets/Heart.jsx",[],{"ruleId":"218","replacedBy":"219"},{"ruleId":"220","replacedBy":"221"},{"ruleId":"222","severity":1,"message":"223","line":30,"column":15,"nodeType":"224","endLine":36,"endColumn":6},{"ruleId":"222","severity":1,"message":"225","line":36,"column":8,"nodeType":"226","endLine":36,"endColumn":18,"suggestions":"227"},{"ruleId":"222","severity":1,"message":"228","line":19,"column":6,"nodeType":"226","endLine":19,"endColumn":8,"suggestions":"229"},{"ruleId":"230","severity":1,"message":"231","line":82,"column":13,"nodeType":"232","endLine":82,"endColumn":44},{"ruleId":"230","severity":1,"message":"231","line":93,"column":13,"nodeType":"232","endLine":93,"endColumn":44},{"ruleId":"230","severity":1,"message":"231","line":104,"column":13,"nodeType":"232","endLine":104,"endColumn":44},{"ruleId":"233","severity":1,"message":"234","line":2,"column":26,"nodeType":"235","messageId":"236","endLine":2,"endColumn":36},{"ruleId":"233","severity":1,"message":"237","line":9,"column":18,"nodeType":"235","messageId":"236","endLine":9,"endColumn":37},{"ruleId":"233","severity":1,"message":"238","line":12,"column":8,"nodeType":"235","messageId":"236","endLine":12,"endColumn":19},{"ruleId":"239","severity":1,"message":"240","line":118,"column":25,"nodeType":"241","messageId":"242","endLine":118,"endColumn":27},{"ruleId":"222","severity":1,"message":"243","line":258,"column":6,"nodeType":"226","endLine":258,"endColumn":19,"suggestions":"244"},{"ruleId":"222","severity":1,"message":"245","line":297,"column":6,"nodeType":"226","endLine":297,"endColumn":