 (userData) { \n            axios\n                .patch(`http://localhost:5000/listings/${listingId}/likeListing`, userId)\n                .then((response) => {\n                    setListingData(response.data)\n                    console.log('(un)liked & listing data updated')\n                })\n        } else {\n            setToggle(true)\n            setTimeout(() => {\n                setToggle(false)\n            }, 2500)\n        }\n    }\n\n    \n    \n    return (\n        <div className=\"listing\">\n            {/* listing image */}\n            <div className=\"listing-img-div\">\n                <img src={listing.image1} />\n            </div>\n            {/* listing tooltip for like and comment */}\n            <div className=\"listing-tooltip\">\n                \n                    <button className=\"listing-tooltip-like p-1\" onClick={handleLike}>\n                        <img src={like} />\n                    </button>\n                    <span><h5 className=\"p-1\">{listingData.likers.length}</h5></span>\n                \n                \n                    <button className=\"listing-tooltip-comment p-1\">\n                    <img src={comment} />\n                    </button>\n                    <span><h5 className=\"p-1\">{listing.commentCount}</h5></span>\n                    <Tooltip content=\"Please sign in to like and comment\" toggle={toggle} setToggle={setToggle}/>\n            </div>\n            {/* title, price, details btn */}\n            <div className=\"listing-title\">\n                <h3>{listing.title}</h3>\n            </div>\n            <div className=\"listing-price\">\n                <h3>${listing.price}</h3>\n            </div>\n            <div className=\"listing-btn\">\n                {/* <Link to={`listings/${listingId}`}> */}\n                    <button className=\"button-primary\" \n                        onClick={() => {\n                        history.push(`/listings/${listingId}`)\n                        }}\n                    >Details</button>\n                {/* </Link> */}\n            </div>\n        </div>\n    )\n}\n\nexport default Listing\n","/Users/eskimo/Coding/Lime-market/client/src/components/Loading.js",[],"/Users/eskimo/Coding/Lime-market/client/src/components/LoginButton.js",[],"/Users/eskimo/Coding/Lime-market/client/src/components/LogoutButton.js",["164"],"import { useAuth0 } from \"@auth0/auth0-react\";\n\nconst LogoutButton = (props) => {\n  return (\n    <button className=\"logout-btn\" onClick={props.logoutAll}>\n      Log Out\n    </button>\n  );\n};\n\nexport default LogoutButton;","/Users/eskimo/Coding/Lime-market/client/src/components/MenuButton.js",[],"/Users/eskimo/Coding/Lime-market/client/src/components/ListingForm.js",["165","166","167","168","169","170","171","172","173","174","175"],"/Users/eskimo/Coding/Lime-market/client/src/api/index.js",[],"/Users/eskimo/Coding/Lime-market/client/src/routes/SignupPage.js",["176","177"],{"ruleId":"178","replacedBy":"179"},{"ruleId":"180","replacedBy":"181"},{"ruleId":"182","severity":1,"message":"183","line":1,"column":17,"nodeType":"184","messageId":"185","endLine":1,"endColumn":25},{"ruleId":"182","severity":1,"message":"186","line":2,"column":8,"nodeType":"184","messageId":"185","endLine":2,"endColumn":14},{"ruleId":"182","severity":1,"message":"187","line":14,"column":23,"nodeType":"184","messageId":"185","endLine":14,"endColumn":34},{"ruleId":"182","severity":1,"message":"188","line":15,"column":36,"nodeType":"184","messageId":"185","endLine":15,"endColumn":50},{"ruleId":"189","severity":1,"message":"190","line":16,"column":17,"nodeType":"191","endLine":16,"endColumn":59},{"ruleId":"189","severity":1,"message":"190","line":47,"column":17,"nodeType":"191","endLine":47,"endColumn":59},{"ruleId":"182","severity":1,"message":"192","line":3,"column":26,"nodeType":"184","messageId":"185","endLine":3,"endColumn":36},{"ruleId":"182","severity":1,"message":"193","line":5,"column":10,"